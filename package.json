{
  "name": "MOBB",
  "private": true,
  "description": "The MOBB: Map of Black Businesses",
  "main": "main.js",
  "scripts": {
    "start": "meteor run --settings settings-dev.json",
    "local": "meteor run --production --settings settings-prod.json",
    "idev": "meteor run ios-device --settings settings-dev.json",
    "stage": "meteor deploy mobbtest.meteorapp.com --settings settings-dev.json",
    "deploy": "meteor deploy mobb.meteorapp.com --owner tenksolutions --settings settings-prod.json",
    "ibuild": "meteor build /Users/KHB/Sites/MOBBapp/testbuild --server localhost:3000 --mobile-settings settings-prod.json",
    "istage": "meteor build /Users/KHB/Sites/MOBBapp/stages --server https://mobbtest.meteorapp.com --mobile-settings settings-prod.json",
    "ideploy": "meteor build /Users/KHB/Sites/MOBBapp/build --server https://www.mobb.biz --mobile-settings settings-prod.json"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
